<% include ../templates/header %>

<article style="padding:24px;">
  <form>
    <div class="form-group">
      <label for="gate">Gerbang</label>
      <input type="text" name="gate" id="gate" placeholder="D03" maxlength="3">
    </div>

    <div class="form-group">
      <label for="pemesanan">Kota</label>
      <select name="pemesanan" id="pemesanan">
        <% for (let pemesanan of pemesanans) { %>
          <option value="<%= pemesanan.id_pemesanan %>">
            <%= pemesanan.id_pemesanan + ' - ' + pemesanan.nama_pemesan + ' - ' + pemesanan.tanggal_pemesanan %>
          </option>
        <% } %>
      </select>
    </div>

    <div class="form-group flex column" id="group">
    </div>

    <div class="form-group">
      <button class="btn accent-color" id="simpan">Simpan</button>
    </div>
  </form>
</article>
</section>

<script src="/javascripts/jquery.js"></script>
<script src="/javascripts/main.js"></script>
<script src="/javascripts/checkin/index.js"></script>
</main>
</body>

</html>